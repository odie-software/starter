{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .vue,.js,.ts",
    "lint:fix": "eslint . --ext .vue,.js,.ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.5.2",
    "@pinia/nuxt": "^0.11.1",
    "@quasar/extras": "^1.17.0",
    "h3": "^1.15.3",
    "h3-proxy": "^1.13.0",
    "nuxt": "^3.17.6",
    "nuxt-quasar-ui": "2.1.12",
    "pinia": "^3.0.3",
    "quasar": "^2.18.1",
    "socket": "^1.0.104",
    "socket.io": "^4.8.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.19.2",
    "eslint": "^9.30.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "src/**/*.{js,vue,html,css}": "prettier --write",
    "src/**/*.{js,vue}": "eslint --fix"
  }
}
